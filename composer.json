{
    "type": "library",
    "license": "Apache-2.0",
    "name": "seanbarton/salesforce",
    "description": "A library that provides a simple and intuitive way to interact with Salesforce API",
    "keywords": [
        "sapi",
        "salesforce",
        "rest",
        "api"
    ],
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "seanbarton\\Salesforce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
